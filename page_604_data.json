    "sura_no": 113,
    "sura_name_en": "Al-Falaq",
    "sura_name_ar": "الفَلَق",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 5,
    "aya_text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ ﰄ",
    "aya_text_emlaey": "ومن شر حاسد إذا حسد"
  },
  {
