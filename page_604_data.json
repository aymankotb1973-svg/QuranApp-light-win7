    "sura_no": 113,
    "sura_name_en": "Al-Falaq",
    "sura_name_ar": "الفَلَق",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 2,
    "aya_text": "مِن شَرِّ مَا خَلَقَ ﰁ",
    "aya_text_emlaey": "من شر ما خلق"
  },
  {
    "id": 6228,
    "jozz": 30,
    "page": 604,
    "sura_no": 113,
    "sura_name_en": "Al-Falaq",
    "sura_name_ar": "الفَلَق",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 3,
    "aya_text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ ﰂ",
    "aya_text_emlaey": "ومن شر غاسق إذا وقب"
