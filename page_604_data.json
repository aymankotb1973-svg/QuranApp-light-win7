    "sura_no": 112,
    "sura_name_en": "Al-Ikhlāṣ",
    "sura_name_ar": "الإخلَاص",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "aya_text": "ٱللَّهُ ٱلصَّمَدُ ﰁ",
